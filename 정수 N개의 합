public class Test {
	long sum(int[] a) {
		
		long sum = 0; //a의 합
		
		for(int i = 0; i < a.length; i++) {
			sum += a[i];
		}
		return sum;
		
	}
}

public class Main {
	
	public static void main(String[] args) {
		
		
		class Test {
			long sum(int[] a) {

				long sum = 0;
				
				for(int i = 0; i < a.length; i++) {
					a[i] = i+1;
					sum += a[i];

				}
				return sum;
			}
		}
		int[] arr = new int[5];

		System.out.println(new Test().sum(arr)); 
		
	}
}
